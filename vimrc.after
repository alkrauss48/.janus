colorscheme wombat256mod

set expandtab
set shiftwidth=2

set tabstop=2
set smarttab
set cc=80
hi  ColorColumn ctermbg=red ctermfg=white guibg=#592929
hi Comment ctermfg=grey guifg=#9a9a9a
set lbr
set tw=500

set ai "Auto indent
set si "Smart indet
set nowrap "Don't wrap lines

let NERDTreeShowHidden=1

set splitright

set list
set listchars=tab:▸\ ,eol:¬
highlight NonText     ctermfg=175
highlight SpecialKey  ctermfg=175
